{
  "name": "sports-app",
  "version": "1.0.0",
  "description": "Sports social network",
  "main": "./bin/www",
  "scripts": {
    "test": "npx jasmine",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate-fresh": "npx sequelize-cli db:migrate:undo:all && npx sequelize-cli db:migrate",
    "seed": "npx sequelize-cli db:migrate:undo:all && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:undo:all && npx sequelize-cli db:seed:all",
    "db": "bash scripts/sequelize.sh",
    "start": "npx sequelize-cli db:migrate && nodemon ./bin/www",
    "production": "pm2 start ecosystem.config.js",
    "worker": "nodemon ./bin/worker",
    "keys": "node ./config/key/generateKeypair",
    "admin": "nodemon ./bin/admin"
  },
  "author": "Patrick Rizzardi",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^10.2.5",
    "bcrypt": "^5.0.0",
    "bull": "^3.29.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "mailgun-js": "^0.22.0",
    "mysql2": "^2.2.5",
    "nodemon": "^2.0.4",
    "sequelize": "^6.3.4",
    "serve-favicon": "^2.5.0",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "eslint": "^7.23.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jasmine": "^3.6.1",
    "prettier": "^2.2.1",
    "sequelize-cli": "^6.2.0",
    "sequelize-mock": "^0.10.2"
  }
}
